{
  "openapi": "3.0.0",
  "info": {
    "title": "JewelVault Backend API",
    "description": "RESTful API for JewelVault - Jewelry Management System (Legacy/Test Endpoints Only)",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:8000",
      "description": "Development"
    },
     {
      "url": "https://jewelvaultbackend-ep2l5o74za-el.a.run.app",
      "description": "Production"
    }
  ],
  "paths": {
    "/": {
      "get": {
        "summary": "Root endpoint",
        "operationId": "root_message",
        "responses": {
          "200": {
            "description": "Welcome message"
          }
        }
      }
    },
    "/ping/": {
      "get": {
        "summary": "Health check",
        "operationId": "ping_view",
        "responses": {
          "200": {
            "description": "Status OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/users/": {
      "get": {
        "summary": "List users from Firestore",
        "operationId": "list_users",
        "responses": {
          "200": {
            "description": "List of users"
          }
        }
      }
    },
    "/vault-samples/": {
      "get": {
        "summary": "List vault samples from PostgreSQL",
        "operationId": "list_vault_samples",
        "responses": {
          "200": {
            "description": "List of vault samples"
          }
        }
      }
    },
    "/vault-samples/seed/": {
      "get": {
        "summary": "Get vault sample seed status",
        "operationId": "get_seed",
        "responses": {
          "200": {
            "description": "Seed status"
          }
        }
      },
      "post": {
        "summary": "Seed vault samples table",
        "operationId": "seed_vault_samples",
        "responses": {
          "200": {
            "description": "Seed operation result"
          }
        }
      }
    }
  }
}